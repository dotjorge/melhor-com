// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  // url      = env("DATABASE_URL")
  url      = "file:./dev.db"
}

model Phone {
  code      String   @id @default(uuid())
  model     String
  price     Decimal
  brand     String
  color     String
  startDate DateTime
  endDate   DateTime

  @@map("phones")
}
